<%= form_for [@restaurant, @review] do |f| %>
  <%= f.label :Name %>
  <%= f.text_field :name %>
  <%= f.label :Address %>
  <%= f.text_field :address %>
  <%= f.label :Phone_Number %>
  <%= f.text_field :phone_number %>
  <%= f.label :Category %>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>
